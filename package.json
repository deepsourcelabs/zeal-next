{
  "name": "zeal-next",
  "version": "0.0.0",
  "main": "./dist/zeal-next.js",
  "module": "./dist/zeal-next.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/zeal-next.mjs",
      "require": "./dist/zeal-next.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "start-storybook -p 6006",
    "build": "vue-tsc --declaration --emitDeclarationOnly --project tsconfig.build.json && vite build",
    "build:storybook": "build-storybook",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.1",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@rushstack/eslint-patch": "^1.1.0",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/builder-vite": "^0.1.27",
    "@storybook/vue3": "^6.4.21",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.26",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.8.1",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.33.9"
  }
}
